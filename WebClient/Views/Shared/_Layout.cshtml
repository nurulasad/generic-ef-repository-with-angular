<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/dataTables/media/js/jquery.dataTables.min.js"></script>

    <script src="~/Scripts/angular-datatables.min.js"></script>
    <script src="~/Scripts/tableApp.js"></script>
    <script src="~/Scripts/tableController.js"></script>

    <!-- main stylesheet -->
    <link rel="stylesheet" href="~/Scripts/dataTables/media/css/jquery.dataTables.min.css" media="all" id="main_stylesheet">

</head>
<body>
   
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>


    @RenderSection("scripts", required: false)
</body>
</html>
